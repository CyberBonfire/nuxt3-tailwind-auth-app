{
  "name": "nuxt3-tailwind-auth-app",
  "version": "0.0.2",
  "description": "An example app using Nuxt3 with TailwindCSS.",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cymagix/nuxt3-tailwind-auth-app.git"
  },
  "author": "cymagix",
  "private": true,
  "scripts": {
    "dev": "nuxi dev -o",
    "tailwind": "tailwind-config-viewer -o -p 4000",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css,vue}'",
    "lint": "eslint . --ext .vue,.ts",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/vue-fontawesome": "3.0.0-5",
    "@nuxt/vite-builder": "^3.0.0-rc.11",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "10.4.12",
    "axios": "^1.0.0",
    "blueimp-md5": "2.19.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "nuxt": "3.0.0-rc.11",
    "postcss": "8.4.17",
    "prettier": "^2.7.1",
    "sass": "1.55.0",
    "tailwind-config-viewer": "1.7.2",
    "tailwindcss": "3.1.8",
    "typescript": "^4.8.4",
    "vee-validate": "4.6.10",
    "vite-plugin-eslint": "^1.8.1",
    "yup": "0.32.11",
    "zxcvbn": "4.4.2"
  }
}
